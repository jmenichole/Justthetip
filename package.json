{
  "name": "justthetip",
  "version": "1.0.0",
  "description": "Professional Solana Smart Contract SDK for enterprise Discord bot development",
  "main": "bot_smart_contract.js",
  "homepage": "https://jmenichole.github.io/Justthetip",
  "scripts": {
    "start": "node api/server.js",
    "start:bot": "npm run verify-env -- --smart-contract && node bot_smart_contract.js",
    "start:bot-railway": "node scripts/start-bot-railway.js",
    "start:smart-contract": "npm run verify-env -- --smart-contract && node bot_smart_contract.js",
    "start:passkey-backend": "cd rust-backend && cargo run",
    "build:passkey-backend": "cd rust-backend && cargo build --release",
    "test": "jest",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "audit": "npm audit",
    "demo:sdk": "node contracts/example.js",
    "verify-env": "node scripts/verify-env.js",
    "verify-railway-secrets": "node scripts/verify-railway-secrets.js",
    "build:contracts": "cd justthetip-contracts && anchor build",
    "test:contracts": "cd justthetip-contracts && anchor test",
    "deploy:devnet": "cd justthetip-contracts && anchor deploy --provider.cluster devnet",
    "deploy:mainnet": "cd justthetip-contracts && anchor deploy --provider.cluster mainnet",
    "clean": "node scripts/cleanup.js",
    "clean:deep": "node scripts/cleanup.js --deep",
    "security-check": "node scripts/security-check.js",
    "version": "node scripts/version.js",
    "version:patch": "node scripts/version.js patch",
    "version:minor": "node scripts/version.js minor",
    "version:major": "node scripts/version.js major",
    "release": "node scripts/release.js",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "organize-docs": "node scripts/organize-docs.js",
    "setup-hooks": "node scripts/setup-hooks.js",
    "kick-setup": "node scripts/kick-setup.js",
    "validate": "npm run lint && npm test && npm run security-check"
  },
  "author": "",
  "license": "Custom",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "@magic-ext/bitcoin": "^26.2.0",
    "@magic-ext/flow": "^26.2.0",
    "@magic-ext/solana": "^28.2.0",
    "@magic-sdk/admin": "^2.8.2",
    "@metaplex-foundation/js": "^0.20.1",
    "@payai/x402-solana": "^0.1.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.98.4",
    "@sqds/sdk": "^2.0.4",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/solana-adapter": "^0.0.8",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.4.1",
    "bs58": "^6.0.0",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.3",
    "dotenv-safe": "^8.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "magic-sdk": "^31.2.0",
    "pm2": "^6.0.10",
    "rate-limiter-flexible": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "stripe": "^19.3.1",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/jest": "^29.5.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^2.1.0",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.4"
  },
  "resolutions": {
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "ws": "^7.5.9"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
